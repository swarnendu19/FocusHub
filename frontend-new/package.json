{
  "name": "frontend-new",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true bun run build",
    "build:debug": "NODE_ENV=development bun run build",
    "build:prod": "NODE_ENV=production bun run build",
    "build:staging": "NODE_ENV=staging bun run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test --project=visual-regression",
    "test:performance": "playwright test --project=performance",
    "test:accessibility": "playwright test --project=accessibility",
    "test:compatibility": "playwright test --grep=compatibility",
    "test:api": "playwright test --project=api-integration",
    "test:all": "bun run test:run && bun run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --reporter=verbose --coverage",
    "test:a11y": "vitest run --config=vitest.a11y.config.ts",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "lighthouse": "lhci autorun",
    "lighthouse:desktop": "lhci autorun --config=.lighthouserc.json",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules",
    "prepare": "husky install || true",
    "postinstall": "bun run playwright:install || true"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "next": "16.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "axe-playwright": "^2.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^8.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.40.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  }
}
